#include <stdio.h>
int main ()
{
	float a,b,c;
	printf("Just enter whatever you want, as long as it's a number");
	scanf("%f %f %f", &a,&b,&c);
	
	if(a>b and a>c){
			printf ("Heroes Win");
	}else
	{
		if(b>a and b>c){
			printf ("Villains Win");
		}else
		{
			if(c>a and c>b){
				printf ("Who Won?");
			}
		}
	}

	return 0;
}
